# ─── Database ───────────────────────────────────────────────
DATABASE_URL=postgresql+asyncpg://agentflow:agentflow@localhost:5432/agentflow
SYNC_DATABASE_URL=postgresql://agentflow:agentflow@localhost:5432/agentflow

# ─── LLM Keys — framework auto-detects provider from prefix ─
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...
GEMINI_API_KEY=AIza...
GROQ_API_KEY=gsk_...
COHERE_API_KEY=
MISTRAL_API_KEY=
TOGETHER_API_KEY=
PERPLEXITY_API_KEY=pplx-...
FIREWORKS_API_KEY=fw_...
DEEPSEEK_API_KEY=
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_DEPLOYMENT=gpt-4o

# ─── Active model (framework picks best available if blank) ─
ACTIVE_LLM_MODEL=

# ─── Ollama / vLLM local ────────────────────────────────────
OLLAMA_BASE_URL=http://localhost:11434
VLLM_BASE_URL=http://localhost:8000

# ─── App ────────────────────────────────────────────────────
APP_ENV=development
LOG_LEVEL=INFO
SECRET_KEY=change-me-in-prod
CORS_ORIGINS=http://localhost:5173
